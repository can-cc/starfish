{
  "name": "@starfishx/cli",
  "version": "1.0.12",
  "description": "blog render tool",
  "main": "index.js",
  "scripts": {
    "precompile": "npm run clean",
    "clean": "rm -rf built",
    "compile": "tsc",
    "test": "jest",
    "watch": "tsc --watch",
    "prepublish": "npm run compile"
  },
  "bin": {
    "starfish": "bin/starfish"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "built"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/built/*"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.(ts)",
      "<rootDir>/src/**/*.test.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/A-Horse/starfish.git"
  },
  "authorn": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/A-Horse/starfish/issues"
  },
  "homepage": "https://github.com/A-Horse/starfish",
  "dependencies": {
    "bluebird": "^3.3.5",
    "blueimp-md5": "^2.3.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-spawn": "^2.2.3",
    "dateformat": "^1.0.12",
    "ejs": "^2.5.7",
    "fs-extra": "^0.26.5",
    "glob": "^7.0.3",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.20",
    "marked": "^4.0.10",
    "meow": "^3.3.0",
    "moment": "^2.24.0",
    "ora": "^1.2.0",
    "orgpr": "^0.2.10",
    "ramda": "^0.21.0",
    "rimraf": "^2.6.2",
    "shelljs": "^0.7.8"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^14.14.5",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.3",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^4.0.5"
  },
  "author": ""
}
